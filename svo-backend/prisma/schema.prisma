generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model menu {
  id Int @id @default(autoincrement())
  name_button String
  message String? @db.LongText
}

model faq {
  id Int @id @default(autoincrement())
  name_button String
  question String @db.LongText
  answer String @db.LongText
}

model fund {
  id Int @id @default(autoincrement())
  name_button String
  message String? @db.LongText
}

model feedback {
  id Int @id @default(autoincrement())
  name_button String
  type String
  message String? @db.LongText
}
