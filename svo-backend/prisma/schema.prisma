generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model menu {
  id Int @id @default(autoincrement())
  title String
  options String @unique
}
model faq {
  id Int @id @default(autoincrement())
  question String
  answer String
}

model feedback {
  id Int @id @default(autoincrement())
  type String
  title String
  message String
}
