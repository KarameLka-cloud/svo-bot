// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model screens {
  id      Int    @id @default(autoincrement())
  code    String @unique
  message String @db.LongText
}

model menu {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  message     String? @db.LongText
  url         String? @db.LongText
}

model want {
  id            Int              @id @default(autoincrement())
  type          String
  name_button   String
  message       String?          @db.LongText
  url           String?          @db.LongText
  wantRelations want_relations[]
}

model want_relations {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  message     String? @db.LongText
  url         String? @db.LongText
  want        want    @relation(fields: [want_id], references: [id])
  want_id     Int
}

model help {
  id            Int              @id @default(autoincrement())
  type          String
  name_button   String
  message       String?          @db.LongText
  url           String?          @db.LongText
  helpRelations help_relations[]
}

model help_relations {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  message     String? @db.LongText
  url         String? @db.LongText
  help        help    @relation(fields: [help_id], references: [id])
  help_id     Int
}

model employment {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  message     String? @db.LongText
  url         String? @db.LongText
}

model faq {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  question    String  @db.LongText
  answer      String  @db.LongText
  url         String? @db.LongText
}

model fund {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  message     String? @db.LongText
  url         String? @db.LongText
}

model feedback {
  id          Int     @id @default(autoincrement())
  type        String
  name_button String
  message     String? @db.LongText
  url         String? @db.LongText
}
